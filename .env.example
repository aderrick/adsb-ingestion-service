# Environment Variables for ADS-B Ingestion Service
# Copy this file to .env and update with your values

# Dump1090 Settings
DUMP1090_HOST=localhost
DUMP1090_PORT=30003

# Database Settings
DB_HOST=localhost
DB_PORT=3306
DB_NAME=adsb
DB_USER=adsb_user
DB_PASSWORD=change_this_password

# Logging
LOG_LEVEL=INFO
LOG_FILE=/var/log/adsb-ingestion/service.log